{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1.7",
    "ext-gd": "*",
    "barryvdh/laravel-dompdf": "^2.0",
    "biscolab/laravel-recaptcha": "^6.0",
    "elasticsearch/elasticsearch": "^8.8",
    "google/apiclient": "^2.13",
    "guzzlehttp/guzzle": "^7.0.1",
    "haruncpi/laravel-simple-captcha": "^1.0",
    "http-interop/http-factory-guzzle": "^1.2",
    "illuminate/support": "v10.13.1",
    "jackiedo/cart": "^3.0",
    "laravel/framework": "^v10.13.1",
    "laravel/sanctum": "^3.2",
    "laravel/scout": "^10.2",
    "laravel/slack-notification-channel": "^2.5",
    "laravel/socialite": "^5.6",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^4.2",
    "livewire/livewire": "^2.12",
    "meilisearch/meilisearch-php": "^1.1",
    "realrashid/sweet-alert": "^7.0",
    "spatie/laravel-permission": "^5.8",
    "teamtnt/tntsearch": "^3.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.7",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^v7.5.2",
    "phpunit/phpunit": "^10.2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
